FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     icoFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;
/* stopAt          writeNow; */

endTime         0.005;

deltaT          2.5E-05; //0.005;

writeControl    timeStep;

writeInterval   2;

purgeWrite      20;

writeFormat     binary;

writePrecision  12;

writeCompression on;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

